{
  "rules": {
    "aa14fdd9": {
      "users": {
        "$uid": {
          ".read": "auth != null && $uid === auth.uid",
          ".write": "auth != null && $uid === auth.uid"
        }
      }
    },
    "b6d6cc89": {
      "slogs": {
        "$usertoken": {
          ".read": false,
          ".write": true
        }
      }
    },
    "ce471190": {
      "chats": {
        "$chatroomid": {
          // TODO: fix: add a proper validation rule
          ".read": "auth !== null && $chatroomid.contains(auth.uid)",
          ".write": "auth !== null && $chatroomid.contains(auth.uid)",
          ".validate": "$chatroomid.beginsWith(auth.uid) ? ( auth.uid < $chatroomid.replace(auth.uid + ':u1:u2:', '') ) : ( $chatroomid.replace(':u1:u2:' + auth.uid, '') < auth.uid )"
        }
      }
    }
  }
}
